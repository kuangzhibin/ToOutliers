/**index.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.section{
  margin-bottom: 10px;
  padding: 0 10px;
}
.addTodo{
  border: 1px solid #096;
  padding: 10px;
  margin-bottom: 10px;
}
.picker{
  background: #096;
  color: #fff;
  padding: 10px;
  text-align: center
}
.crossLine{
  text-decoration: line-through;
}
.list{
  padding: 10px;
  margin: 0 0 10px;
  background: #096;
  color: #fff;
}